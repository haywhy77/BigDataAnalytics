<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Timing Statistics</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            border: 1px solid #ccc;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f4f4f4;
        }
    </style>
</head>
<body>
    <h1>Timing Statistics</h1>
    <table>
        <thead>
            <tr>
                <th>File Name</th>
                <th>Elapsed Time (ms)</th>
                <th>Number of Lines</th>
                <th>Normalized Time (ms/line)</th>
            </tr>
        </thead>
        <tbody>
            <% processingTimes.forEach(stat => { %>
                <tr>
                    <td><%= stat.fileName %></td>
                    <td><%= stat.elapsedTime.toFixed(2) %></td>
                    <td><%= stat.lineCount %></td>
                    <td><%= stat.normalizedTime.toFixed(2) %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>